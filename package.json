{
  "name": "duplicate-file-detector",
  "version": "0.1.0",
  "description": "A fast, efficient desktop app for detecting and removing duplicate files",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
